// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())@unique
  Name      String   
  Email     String   @unique
  Pass      String
  Cellphone Int      @unique
  Admin     Boolean
}

model Consult{
  id        Int      @id @default(autoincrement())@unique
  Operation String
  Day       String
  Time      String
  Client    String
  Dentist   String   @unique
}